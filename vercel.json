{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "installCommand": "npm run install-modules",
                "buildCommand": "npm run build-all",
                "outputDirectory": "back-end/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
